                    <h5>Клавиатура:</h5>
                </div>
                <div class="col-lg-7">
                    @Session["KeyboardName"]
                </div>
                <div class="col-lg-1">
                    <h5>@Session["KeyboardPrice"]</h5>
                </div>

                <div class="col-lg-1">
                    @if (@Session["keyboardId"] == null || @Session["keyboardId"].ToString() == "")
                    {
                    }
                    else
                    {
                        @Html.ActionLink("Убрать", "deleteComplect", new { id = Session["keyboardId"] }, new { @class = "btn btn-primary", @style = "color:white" })
                    }
                </div>

                <div class="col-lg-1">
                    <button class="btn-primary btn-wrapper-end" id="example_10_2" onclick=" document.getElementById('KeyboardTable').style.display = 'none';
                                    document.getElementById('example_10_2').style.display = 'none';
                                    document.getElementById('example_10_1').style.display = 'block';"
                            style="display: none; text-align: end">
                        Скрыть
                    </button>
                    <button class="btn-primary btn-wrapper-end" id="example_10_1" onclick="document.getElementById('KeyboardTable').style.display = 'block';
                                    document.getElementById('example_10_1').style.display = 'none';
                                    document.getElementById('example_10_2').style.display = 'block';"
                            style=" text-align:end">
                        Показать
                    </button>
                </div>
            </div>
        </div>
        <div class="container-fluid" id="KeyboardTable" runat="server" style="display:none">


            @foreach (var item in Model)
            {
                if (item.type == "keyboard")
                {
                    <div class="card mb-3" style="margin-top: 10px; margin-left: 100px; border-style: solid; border-color: lightgray; border-width: 2px; border-radius: 4px;">
                        <div class="row g-0">
                            <div class="col-md-2">
                                <img src="~/images/keyboard.jpg" alt="..." width="100" height="100">
                            </div>
                            <div class="col-md-2">
                                <div class="card-body">
                                    <h4 class="card-title">@Html.DisplayFor(modelItem => item.name)</h4>
                                    <p class="card-text">Цена: @Html.DisplayFor(modelItem => item.price) руб.</p>
                                </div>
                            </div>

                            <div class="col-md-8" style="text-align:end; margin-top:26px;">
                                @Html.ActionLink("Подробнее", "Details", new { id = item.id }, new { @class = "btn btn-primary compItem", @style = "color:white" })
                                @Html.ActionLink("Добавить", "addComplect", new { id = item.id }, new { @class = "btn btn-primary ", @style = "color:white" })
                            </div>

                        </div>
                    </div>
                }
            }
        </div>


        <div class="row">
            <div class="col-lg-9" style="text-align: start;">
                <h3>Общая сумма: @Session["ResultSum"]</h3>
            </div>
            <div class="col-lg-2" style="text-align: end;">
                @Html.ActionLink("Заказать", "Create",null, new { @class = "btn btn-primary compItem2", @style = "color:white" })
            </div>

        </div>
    </div>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")


    @section scripts
{
        <script type="text/javascript">

            $(function () {
                $.ajaxSetup({ cache: false });
                $(".compItem").click(function (e) {

                    e.preventDefault();
                    $.get(this.href, function (data) {
                        $('#dialogContent').html(data);
                        $('#modDialog').modal('show');
                    });
                });
            })
        </script>

        <script type="text/javascript">

            $(function () {
                $.ajaxSetup({ cache: false });
                $(".compItem2").click(function (e) {

                    e.preventDefault();
                    $.get(this.href, function (data) {
                        $('#dialogContent2').html(data);
                        $('#modDialog2').modal('show');
                    });
                });
            })
        </script>
    }

</body>


